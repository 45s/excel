{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true,
    "jest/globals": true
  },
  "extends": ["eslint:recommended", "google", "prettier"],
  "plugins": ["jest", "prettier"],
  "rules": {
    "prettier/prettier": 1,
    "semi": 0,
    "arrow-parens": 0,
    "comma-dangle": 0,
    "require-jsdoc": 0
  }
}
